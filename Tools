HopperBin195.Name = "Teleport"
HopperBin195.Parent = mas
Script196.Name = "TeleportScript"
Script196.Parent = HopperBin195
table.insert(cors,sandbox(Script196,function()
local COOLDOWN = 0
local MP = 0
bin = script.Parent
function TryToCast(player)
	local stats = player:findFirstChild("leaderstats")
	if stats == nil then return false end
	local mana = stats:findFirstChild("Mana")
	local level = stats:findFirstChild("Level")
	if mana == nil or level == nil then return false end

	if (mana.Value >= MP) then
		mana.Value = mana.Value - MP
		return true
	end

	return false
end




function teleportPlayer(pos)
local player = game.Players.LocalPlayer
if player == nil or player.Character == nil then return end
player.Character.Torso.CFrame = CFrame.new(Vector3.new(pos.x, pos.y + 7, pos.z))
end


enabled = true
function onButton1Down(mouse)
	if not enabled then
		return
	end

	local player = game.Players.LocalPlayer
	if player == nil then return end
	

	enabled = false
	mouse.Icon = "rbxasset://textures\\ArrowFarCursor.png"
	local cf = mouse.Hit
	local v = cf.lookVector

	teleportPlayer(cf.p)

	wait(COOLDOWN)
	mouse.Icon = "rbxasset://textures\\ArrowCursor.png"
	enabled = true

end

function onSelected(mouse)
	mouse.Icon = "rbxasset://textures\\ArrowCursor.png"
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

bin.Selected:connect(onSelected)


end))
HopperBin197.Name = "Tra/Ref"
HopperBin197.Parent = mas
LocalScript198.Name = "Main"
LocalScript198.Parent = HopperBin197
table.insert(cors,sandbox(LocalScript198,function()
local bin = script.Parent
local Config = bin:findFirstChild("Config")
if not Config then
	Config = Instance.new("Configuration")
	Config.Name = "Config"
	local __BASE = Instance.new("BoolValue")
	local EL = __BASE:Clone()
	EL.Name = "EnableLocked"
	EL.Value = false
	EL.Parent = Config
	local CU = __BASE:Clone()
	CU.Name = "CanUndo"
	CU.Value = true
	CU.Parent = Config
	local CR = __BASE:Clone()
	CR.Name = "CanReset"
	CR.Value = true
	CR.Parent = Config
	Config.Parent = bin
end
local LockedPartEditingEnabled = Config.EnableLocked.Value
local CanUndoChangesToParts = Config.CanUndo.Value
local CanResetPartValues = Config.CanReset.Value

local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PGui = Player:findFirstChild("PlayerGui")

local CurrentGui;
local Selection;
local SelectionBox = Instance.new("SelectionBox", PGui)
SelectionBox.Name = "Selecty"
local OtherBox = Instance.new("SelectionBox", PGui)
OtherBox.Name = "Selected"
OtherBox.Color = BrickColor.Green()

local CanEdit = false
local Mode = 0 --0 for trans, 1 for refl

function Round(num)
	local nNum = num * 10
	nNum = nNum + 0.5
	nNum = math.floor(nNum)
	return nNum/10
end

function MakeGui(PlayerGui)
	local Main = Instance.new("ScreenGui", PlayerGui)
	local BaseFrame = Instance.new("Frame", Main)
	BaseFrame.Name = "MAIN"
	BaseFrame.BackgroundColor3 = Color3.new()
	BaseFrame.BackgroundTransparency = 0.2
	BaseFrame.BorderColor3 = Color3.new(1,1,1)
	BaseFrame.Position = UDim2.new(0.0, 0, 0.8, -75)
	BaseFrame.Size = UDim2.new(0.2, 0, 0.1, 0)
	local PartName = Instance.new("TextLabel", BaseFrame)
	PartName.Name = "PartLabel"
	PartName.Text = "No part"
	PartName.FontSize = "Size9"
	PartName.Position = UDim2.new(0.5, 0, 0.15, 0)
	PartName.TextColor3 = Color3.new(1,1,1)
	local TransTxt = Instance.new("TextLabel", BaseFrame)
	TransTxt.Name = "TransLabel"
	TransTxt.Text = "Trans - nil"
	TransTxt.FontSize = "Size9"
	TransTxt.Position = UDim2.new(0.5, 0, 0.15, 12)
	TransTxt.TextColor3 = Color3.new(1,1,1)
	local ReflTxt = Instance.new("TextLabel", BaseFrame)
	ReflTxt.Name = "ReflLabel"
	ReflTxt.Text = "Refl - nil"
	ReflTxt.FontSize = "Size9"
	ReflTxt.Position = UDim2.new(0.5, 0, 0.15, 24)
	ReflTxt.TextColor3 = Color3.new(1,1,1)
	local Bar = Instance.new("Frame", BaseFrame)
	Bar.Name = "Bar"
	Bar.BackgroundColor3 = Color3.new()
	Bar.BorderColor3 = Color3.new(1,1,1)
	Bar.Size = UDim2.new(0.8, 0, 0.2, 0)
	Bar.Position = UDim2.new(0.1, 0, 0.8, 0)
		local Fill = Instance.new("Frame", Bar)
		Fill.Name = "Fill"
		Fill.BorderSizePixel = 0
		Fill.BackgroundColor3 = Color3.new(1,1,1)
		Fill.Size = UDim2.new(0.0, 0, 1.0, 0)
		local Indicator = Instance.new("TextLabel", Bar)
		Indicator.Name = "Indicator"
		Indicator.Position = UDim2.new(0.5, 0, 0.5, 0)
		Indicator.Text = "0"
		Indicator.TextColor3 = Color3.new(0,1,0)
	local PlusButton = Instance.new("TextButton", BaseFrame)
	PlusButton.Name = "PlusBtn"
	PlusButton.Text = "+"
	PlusButton.TextColor3 = Color3.new(1,1,1)
	PlusButton.BackgroundColor3 = Color3.new()
	PlusButton.BorderColor3 = Color3.new(1,1,1)
	PlusButton.Size = UDim2.new(0.1, 0, 0.2, 0)
	PlusButton.Position = UDim2.new(0.9, 0, 0.8, 0)
	local MinusButton = Instance.new("TextButton", BaseFrame)
	MinusButton.Name = "MinusBtn"
	MinusButton.Text = "-"
	MinusButton.TextColor3 = Color3.new(1,1,1)
	MinusButton.BackgroundColor3 = Color3.new()
	MinusButton.BorderColor3 = Color3.new(1,1,1)
	MinusButton.Size = UDim2.new(0.1, 0, 0.2, 0)
	MinusButton.Position = UDim2.new(0.0, 0, 0.8, 0)
	MinusButton.MouseButton1Down:connect(function()
		print('MinusBTN')
		if not Selection then return end
		if not CanEdit then return end
		if Mode == 0 then --trans
			local Trans = Selection.Transparency
			if Trans - 0.1 < 0 then
				Trans = 1
			else
				Trans = Trans - 0.1
			end
			Fill:TweenSize(UDim2.new(Round(Trans), 0, 1, 0), "Out", "Quad", 0.5, true, function() Fill.Size = UDim2.new(Trans, 0, 1, 0) end)
			TransTxt.Text = "Trans - " .. Round(Trans)
			Selection.Transparency = Round(Trans)
			Indicator.Text = Round(Trans)
		else --refl
			local Refl = Selection.Reflectance
			if Refl - 0.1 < 0 then
				Refl = 1
			else
				Refl = Refl - 0.1
			end
			Fill:TweenSize(UDim2.new(Round(Refl), 0, 1, 0), "Out", "Quad", 0.5, true, function() Fill.Size = UDim2.new(Refl, 0, 1, 0) end)
			ReflTxt.Text = "Refl - " .. Round(Refl)
			Selection.Reflectance = Round(Refl)
			Indicator.Text = Round(Refl)
		end
	end)
	PlusButton.MouseButton1Down:connect(function()
		print('PlusBTN')
		if not Selection then return end
		if not CanEdit then return end
		if Mode == 0 then --trans
			local Trans = Selection.Transparency
			if Trans + 0.1 > 1 then
				Trans = 0
			else
				Trans = Trans + 0.1
			end
			Fill:TweenSize(UDim2.new(Round(Trans), 0, 1, 0), "Out", "Quad", 0.5, true, function() Fill.Size = UDim2.new(Trans, 0, 1, 0) end)
			TransTxt.Text = "Trans - " .. Round(Trans)
			Selection.Transparency = Round(Trans)
			Indicator.Text = Round(Trans)
		else --refl
			local Refl = Selection.Reflectance
			if Refl + 0.1 > 1 then
				Refl = 0
			else
				Refl = Refl + 0.1
			end
			Fill:TweenSize(UDim2.new(Round(Refl), 0, 1, 0), "Out", "Quad", 0.5, true, function() Fill.Size = UDim2.new(Refl, 0, 1, 0) end)
			ReflTxt.Text = "Refl - " .. Round(Refl)
			Selection.Reflectance = Round(Refl)
			Indicator.Text = Round(Refl)
		end
	end)
	local TransButton = Instance.new("TextButton", BaseFrame)
	TransButton.Name = "TransBtn"
	TransButton.Text = "Trans."
	TransButton.BackgroundColor3 = Color3.new()
	TransButton.BorderColor3 = Color3.new(1,1,1)
	TransButton.Size = UDim2.new(0.4, 0, 0.2, 0)
	TransButton.Position = UDim2.new(0.1, 0, 0.6, 0)
	TransButton.TextColor3 = Color3.new(1,0,0)
	local ReflButton = Instance.new("TextButton", BaseFrame)
	ReflButton.Name = "ReflBtn"
	ReflButton.Text = "Refl."
	ReflButton.BackgroundColor3 = Color3.new()
	ReflButton.BorderColor3 = Color3.new(1,1,1)
	ReflButton.Size = UDim2.new(0.4, 0, 0.2, 0)
	ReflButton.Position = UDim2.new(0.5, 0, 0.6, 0)
	ReflButton.TextColor3 = Color3.new(1,1,1)
	ReflButton.MouseButton1Down:connect(function()
		Mode = 1
		TransButton.TextColor3 = Color3.new(1,1,1)
		ReflButton.TextColor3 = Color3.new(1,0,0)
		if not Selection then return end
		Fill.Size = UDim2.new(Round(Selection.Reflectance), 0, 1, 0)
		Indicator.Text = Round(Selection.Reflectance)
	end)
	TransButton.MouseButton1Down:connect(function()
		Mode = 0
		TransButton.TextColor3 = Color3.new(1,0,0)
		ReflButton.TextColor3 = Color3.new(1,1,1)
		if not Selection then return end
		Fill.Size = UDim2.new(Round(Selection.Transparency), 0, 1, 0)
		Indicator.Text = Round(Selection.Transparency)
	end)
	return Main
end

function SetSelection(item)
	if not CurrentGui then return end
	OtherBox.Adornee = item
	Selection = item
	if not item then
		OtherBox.Adornee = nil
		CurrentGui.MAIN.PartLabel.Text = "No part"
		CurrentGui.MAIN.ReflLabel.Text = "Refl - nil"
		CurrentGui.MAIN.TransLabel.Text = "Trans - nil"
		CanEdit = false
		return
	end
	if not LockedPartEditingEnabled and item.Locked then
		CurrentGui.MAIN.PartLabel.Text = item.Name .. " (No edit)"
		CurrentGui.MAIN.ReflLabel.Text = "Refl - " .. Round(item.Reflectance)
		CurrentGui.MAIN.TransLabel.Text = "Trans - " .. Round(item.Transparency)
		if Mode == 0 then
			CurrentGui.MAIN.Bar.Fill.Size = UDim2.new(Round(item.Transparency),0,1,0)
			CurrentGui.MAIN.Bar.Indicator.Text = Round(item.Transparency)
		else
			CurrentGui.MAIN.Bar.Fill.Size = UDim2.new(Round(item.Reflectance),0,1,0)
			CurrentGui.MAIN.Bar.Indicator.Text = Round(item.Reflectance)
		end
		CanEdit = false
		return
	end
	CurrentGui.MAIN.PartLabel.Text = item.Name
	CurrentGui.MAIN.ReflLabel.Text = "Refl - " .. Round(item.Reflectance)
	CurrentGui.MAIN.TransLabel.Text = "Trans - " .. Round(item.Transparency)
	if Mode == 0 then
		CurrentGui.MAIN.Bar.Fill.Size = UDim2.new(Round(item.Transparency),0,1,0)
		CurrentGui.MAIN.Bar.Indicator.Text = Round(item.Transparency)
	else
		CurrentGui.MAIN.Bar.Fill.Size = UDim2.new(Round(item.Reflectance),0,1,0)
		CurrentGui.MAIN.Bar.Indicator.Text = Round(item.Reflectance)
	end
	CanEdit = true
end

function onClick(mouse)
	SetSelection(mouse.Target)
end

function onMove(mouse)
	SelectionBox.Adornee = mouse.Target
end

function onSelect(mouse)
	print('Selected')
	PGui = Player:findFirstChild("PlayerGui")
	CurrentGui = MakeGui(PGui) --Remade every selection
	SelectionBox.Parent = PGui
	OtherBox.Parent = PGui
	mouse.Move:connect(function() onMove(mouse) end)
	mouse.Button1Down:connect(function() onClick(mouse) end)
end

function onDeselect()
	print('Deselected')
	SelectionBox.Parent = nil
	SelectionBox.Adornee = nil
	OtherBox.Parent = nil
	OtherBox.Adornee = nil
	CurrentGui:Remove()
end

bin.Selected:connect(onSelect)
bin.Deselected:connect(onDeselect)
end))

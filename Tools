table.insert(cors,sandbox(LocalScript163,function()
wait(0.1)

active = false

selectionBox = Instance.new("SelectionBox")
selectionBox.Color = BrickColor.new("Really red")
selectionBox.Visible = true

selectionLasso = Instance.new("SelectionPartLasso")
selectionLasso.Color = BrickColor.Black()
selectionLasso.Humanoid = game.Players.LocalPlayer.Character.Humanoid

gui = Instance.new("ScreenGui")
text = Instance.new("TextLabel")
text.Parent = gui
text.Text = "No brick selected"
text.BackgroundColor3 = Color3.new(51, 153, 255)
text.BorderColor3 = Color3.new(1/256*17, 1/256*17, 1/256*17)
text.TextColor3 = Color3.new(1, 1, 1)
text.Position = UDim2.new(0, 0, 1, -20)
text.Size = UDim2.new(0, 100, 0, 20)
text.BackgroundTransparency = 0.25

function onMouseButton1Down(target)
	if target ~= nil then
		if target.Locked == false then
			if target.CanCollide == true then
				target.CanCollide = false
				selectionBox.Color = BrickColor.Red()
				text.Text = "CanCollide=False"
			elseif target.CanCollide == false then
				text.Text = "CanCollide=True"
				target.CanCollide = true
				selectionBox.Color = BrickColor.Green()
			end
		end
	end
end

function onMouseMoved(target)
	if target ~= nil then
		if target.Locked == false then
			if target.CanCollide == true then
				selectionBox.Adornee = target
				selectionBox.Color = BrickColor.Green()
				selectionLasso.Part = target
				text.Text = "CanCollide=True"
			else
				selectionBox.Adornee = target
				selectionBox.Color = BrickColor.Red()
				selectionLasso.Part = target
				text.Text = "CanCollide=False"
			end
		else
			selectionBox.Adornee = nil
			selectionLasso.Part = nil
			text.Text = "Brick is locked!"
		end
	else
		selectionBox.Adornee = nil
		selectionLasso.Part = nil
		text.Text = "No brick selected"
	end
end

function onSelected(mouse)
	if active == false then
		active = true
		gui.Parent = game.Players.LocalPlayer.PlayerGui
		selectionBox.Parent = game.Players.LocalPlayer.PlayerGui
		selectionLasso.Parent = game.Players.LocalPlayer.PlayerGui
		mouse.Move:connect(function() onMouseMoved(mouse.Target) end)
		mouse.Button1Down:connect(function() onMouseButton1Down(mouse.Target) end)
	end
end

function onDeselected()
	if active == true then
		active = false
		gui.Parent = nil
		selectionBox.Parent = nil
		selectionLasso.Parent = nil
	end
end

script.Parent.Selected:connect(onSelected)
script.Parent.Deselected:connect(onDeselected)

end))
BoolValue164.Name = "MoreTools"
BoolValue164.Parent = HopperBin162
HopperBin165.Name = "Copy"
HopperBin165.Parent = mas
HopperBin165.TextureId = "rbxasset://Textures/Clone.png"
HopperBin165.Active = true
HopperBin165.BinType = Enum.BinType.Clone
HopperBin166.Name = "Delete"
HopperBin166.Parent = mas
HopperBin166.TextureId = "http://www.roblox.com/asset/?id=12223874"
HopperBin166.BinType = Enum.BinType.Hammer
HopperBin167.Name = "Edit Mesh"
HopperBin167.Parent = mas
LocalScript168.Name = "MeshEditScript"
LocalScript168.Parent = HopperBin167
table.insert(cors,sandbox(LocalScript168,function()
MeshScaleValue = Vector3.new(1, 1, 1)
MeshType = "No mesh"
CurrentObject = nil

player = game.Players.LocalPlayer
repeat wait() until player.Character

function drawMenu(obj, list)
	-- obj - the gui object where to draw the menu to
	-- list - a table of strings of what menu items to put. example: {"lol!", "XD", ":D"}
	-- return value: a string of what item you clicked on the menu. if you clicked on "lol!" you would get "lol!"
	local menu = Instance.new("Frame")
	menu.Position = obj.Position
	menu.BackgroundTransparency = 1
	menu.Size = obj.Size
	menu.Parent = obj.Parent

	local menuText = Instance.new("StringValue")
	menuText.Parent = menu
	menuText.Name = "menuText"

	local menuClicked = Instance.new("BoolValue")
	menuClicked.Parent = menu
	menuClicked.Value = false
	menuClicked.Name = "menuClicked"

	for i, v in ipairs(list) do
		local menuItem = Instance.new("TextButton")
		menuItem.Text = v
		menuItem.Parent = menu
		menuItem.Position = UDim2.new(0, 0, -i, 0)
		menuItem.Size = UDim2.new(1.5, 0, 1, 0)
		menuItem.MouseButton1Click:connect(function() onMenuItemClicked(menuItem) end)
	end
	repeat wait() until menuClicked.Value == true
	local value = menuText.Value
	menu:remove()
	return value
end

function onMenuItemClicked(menuItem)
	menuItem.Parent.menuText.Value = menuItem.Text
	menuItem.Parent.menuClicked.Value = true 
end

function onMeshTypeClicked()
	if CurrentObject ~= nil then
		local type = drawMenu(mygui.Frame.MeshType, {"No mesh", "BlockMesh", "CylinderMesh", "Head", "Brick", "Sphere", "Torso", "Wedge"})
		if type == "No mesh" then
			MeshType = type
			for _, v in pairs(CurrentObject:getChildren()) do
				if v.className == "BlockMesh" or v.className == "CylinderMesh" or v.className == "SpecialMesh" then
					v:remove()
				end
			end
		else
			MeshType = type
			changeMesh(type)
		end
		mygui.Frame.MeshType.Text = type
	end
end
			
function onClicked(target)
	if target then
		if not target.Locked then
			CurrentObject = target
			selection.Adornee = CurrentObject
			selection.Color = BrickColor.new("Pastel Blue")
			local foundmesh = false
			for _, v in pairs(CurrentObject:getChildren()) do
				if v.className == "BlockMesh" or v.className == "CylinderMesh" or v.className == "SpecialMesh" then
					MeshScaleValue = v.Scale
					foundmesh = true
				end
				if v.className == "BlockMesh" then
					MeshType = "BlockMesh"
				elseif v.className == "CylinderMesh" then
					MeshType = "CylinderMesh"
				elseif v.className == "SpecialMesh" then
					MeshType = string.sub(tostring(v.MeshType), 15)
				end
			end
			if not foundmesh then
				MeshType = "No mesh"
				MeshScaleValue = Vector3.new(1, 1, 1)
			end
			updateGui(MeshScaleValue)
		else
			CurrentObject = nil
			selection.Adornee = nil
			MeshScaleValue = Vector3.new(1, 1, 1)
			MeshType = "No mesh"
			updateGui(MeshScaleValue)
			selection.Color = BrickColor.Blue()
		end
	else
		CurrentObject = nil
		selection.Adornee = nil
		MeshScaleValue = Vector3.new(1, 1, 1)
		MeshType = "No mesh"
		updateGui(MeshScaleValue)
		selection.Color = BrickColor.Blue()
	end
end

function onMoved(target)
	if not CurrentObject then
		if target then
			if not target.Locked then
				selection.Adornee = target
			else
				selection.Adornee = nil
			end
		else
			selection.Adornee = nil
		end
	end
end

function onSelected(mouse)
	if mygui then
		mygui.Parent = player.PlayerGui
	elseif player.PlayerGui:findFirstChild("MeshGui") == nil then
		mygui = script.Parent.MeshGui:clone()
		mygui.Parent = player.PlayerGui
		selection = Instance.new("SelectionBox")
		selection.Parent = player.PlayerGui
	end
	mouse.Button1Down:connect(function() onClicked(mouse.Target) end)
	mouse.Move:connect(function() onMoved(mouse.Target) end)
	mygui.Frame.MeshType.MouseButton1Click:connect(onMeshTypeClicked)
	mygui.Frame.X.Minus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(-0.1, 0, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.X.Plus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0.1, 0, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.X.Minus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(-1, 0, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.X.Plus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(1, 0, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Y.Minus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, -0.1, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Y.Plus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 0.1, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Y.Minus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, -1, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Y.Plus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 1, 0) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Z.Minus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 0, -0.1) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Z.Plus1.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 0, 0.1) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Z.Minus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 0, -1) updateGui(MeshScaleValue) changeMesh(MeshType) end)
	mygui.Frame.Z.Plus2.MouseButton1Click:connect(function() MeshScaleValue = MeshScaleValue + Vector3.new(0, 0, 1) updateGui(MeshScaleValue) changeMesh(MeshType) end)
end

function onDeselected()
	selection:remove()
	CurrentObject = nil
	MeshScaleValue = Vector3.new(1, 1, 1)
	mygui:remove()
	mygui = nil
end

function changeMesh(mesh)
	if CurrentObject ~= nil then
	local file = ""
		for _, v in pairs(CurrentObject:getChildren()) do
			if v.className == "BlockMesh" or v.className == "CylinderMesh" or v.className == "SpecialMesh" then
				if v.className == "SpecialMesh" then
						file = v.MeshId
				end
				v:remove()
			end
		end
		if mesh == "No mesh" then
			return
		elseif mesh == "BlockMesh" then
			local m = Instance.new("BlockMesh")
			m.Parent = CurrentObject
			m.Scale = MeshScaleValue
		elseif mesh == "CylinderMesh" then
			local m = Instance.new("CylinderMesh")
			m.Parent = CurrentObject
			m.Scale = MeshScaleValue
		else
			local m = Instance.new("SpecialMesh")
			m.MeshType = mesh
			m.MeshId = file
			m.Scale = MeshScaleValue
			m.Parent = CurrentObject
		end
	end
end

function updateGui(scale)
	mygui.Frame.MeshType.Text = MeshType
	mygui.Frame.X.Text.Text = string.format("X: %0.2g", scale.x)
	mygui.Frame.Y.Text.Text = string.format("Y: %0.2g", scale.y)
	mygui.Frame.Z.Text.Text = string.format("Z: %0.2g", scale.z)
end

script.Parent.Selected:connect(onSelected)
script.Parent.Deselected:connect(onDeselected)
end))

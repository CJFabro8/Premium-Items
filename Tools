LocalScript82.Parent = HopperBin81
table.insert(cors,sandbox(LocalScript82,function()
player = game.Players.LocalPlayer
repeat wait() until player.Character

texture = Instance.new("Texture")
decal = Instance.new("Decal")

function onTexInputClicked()
	if mygui.Frame.texInput.Text == "X" then
		mygui.Frame.texInput.Text = ""
	elseif mygui.Frame.texInput.Text == "" then
		mygui.Frame.texInput.Text = "X"
	end
end

function onClicked(target, face)
	if target then
		if not target.Locked then
			surface.Adornee = target
			surface.TargetSurface = face
			if mygui.Frame.texInput.Text == "X" and tonumber(mygui.Frame.idInput.Text) ~= nil and tonumber(mygui.Frame.uInput.Text) ~= nil and tonumber(mygui.Frame.vInput.Text) ~= nil then
				local t = texture:clone()
				t.Parent = target
				t.Face = face
				t.Texture = "http://www.roblox.com/asset/?id="..mygui.Frame.idInput.Text - 1
				t.StudsPerTileU = mygui.Frame.uInput.Text
				t.StudsPerTileV = mygui.Frame.vInput.Text
			elseif mygui.Frame.texInput.Text == "" and tonumber(mygui.Frame.idInput.Text) ~= nil then
				local d = decal:clone()
				d.Parent = target
				d.Face = face
				d.Texture = "http://www.roblox.com/asset/?id="..mygui.Frame.idInput.Text - 1
			end
		end
	end
end

function onMoved(target, face)
	if target then
		if not target.Locked then
			surface.Adornee = target
			surface.TargetSurface = face
			if mygui.Frame.texInput.Text == "X" and tonumber(mygui.Frame.idInput.Text) ~= nil and tonumber(mygui.Frame.uInput.Text) ~= nil and tonumber(mygui.Frame.vInput.Text) ~= nil then
				texture.Parent = target
				texture.Face = face
				texture.Texture = "http://www.roblox.com/asset/?id="..mygui.Frame.idInput.Text - 1
				texture.StudsPerTileU = mygui.Frame.uInput.Text
				texture.StudsPerTileV = mygui.Frame.vInput.Text
			elseif mygui.Frame.texInput.Text == "" and tonumber(mygui.Frame.idInput.Text) ~= nil then
				decal.Parent = target
				decal.Face = face
				decal.Texture = "http://www.roblox.com/asset/?id="..mygui.Frame.idInput.Text - 1
			end
		else
			surface.Adornee = nil
			texture.Parent = nil
			decal.Parent = nil
		end
	else
		texture.Parent = nil
		decal.Parent = nil
		surface.Adornee = nil
	end
end

function onSelected(mouse)
	if mygui then
		mygui.Parent = player.PlayerGui
	elseif player.PlayerGui:findFirstChild("DecalAdderGui") == nil then
		mygui = script.Parent.DecalAdderGui:clone()
		mygui.Parent = player.PlayerGui
		surface = Instance.new("SurfaceSelection")
		surface.Parent = player.PlayerGui
	end
	mouse.Button1Down:connect(function() onClicked(mouse.Target, mouse.TargetSurface) end)
	mouse.Move:connect(function() onMoved(mouse.Target, mouse.TargetSurface) end)
	mygui.Frame.texInput.MouseButton1Click:connect(onTexInputClicked)
end

function onDeselected()
	surface:remove()
	texture:remove()
	decal:remove()
	surface.Adornee = nil
	mygui:remove()
	mygui = nil
end

script.Parent.Selected:connect(onSelected)
script.Parent.Deselected:connect(onDeselected)
end))
